<%- if controller_name != 'sessions' %>
  <%= link_to "Log in", new_session_path(resource_name),class: 'forgot-password' %><br />
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Forgot Password?", new_password_path(resource_name),class: 'forgot-password' %><br />
<% end -%>
